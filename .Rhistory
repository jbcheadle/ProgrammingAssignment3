corr2 <- function(directory, threshold=0) {
# Initialize
csv_files <- list.files(directory, full.names = TRUE)
df <- data.frame()
# Create your data frame
for(i in 1:length(list.files(directory))) {
file <- read.csv(csv_files[i])
nobs <- dim(file[complete.cases(file),])[1]
df <- rbind.data.frame(df,c(i,nobs))
}
colnames(df) <- c("id","nobs")
# Threshold
df<- df[df$nobs>threshold,]
# Generate corr
corrs <- numeric()
for(i in df$id) {
file <- read.csv(csv_files[i])
sulf_nitr <- file[complete.cases(file),][,2:3]
value <- cor(sulf_nitr[,1],sulf_nitr[,2])
corrs <- c(corrs, value)
}
corrs
}
